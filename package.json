{
  "name": "vue-template",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "serve:stage": "vue-cli-service serve --mode staging",
    "serve:prod": "vue-cli-service serve --mode production",
    "build": "vue-cli-service build",
    "build:dev": "vue-cli-service build --mode development",
    "build:stage": "vue-cli-service build --mode staging",
    "style": "stylelint src/**/*.vue src/**/*.scss --fix",
    "stylelint-check": "stylelint-config-prettier-check",
    "lint": " npm run style &&  vue-cli-service lint",
    "commit": "git add -A && git-cz",
    "review": "jscpd ./src",
    "changelog": "conventional-changelog --config ./build/changelog-generator/index.js --infile CHANGELOG.md --same-file",
    "release": "standard-version",
    "release:tag": "npm run release --release-as",
    "release:major": "standard-version --release-as major",
    "release:minor": "standard-version --release-as minor",
    "release:patch": "standard-version --release-as patch",
    "release:first": "standard-version --first-release",
    "release:rc": "standard-version --prerelease rc",
    "release:beta": "standard-version --prerelease beta",
    "release:alpha": "standard-version --prerelease alpha"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/GATING/vue-template.git"
  },
  "dependencies": {
    "axios": "^0.20.0",
    "core-js": "^3.6.5",
    "vue": "^2.6.12",
    "vue-router": "^3.4.4",
    "vuex": "^3.5.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@vue/cli-plugin-babel": "^4.5.6",
    "@vue/cli-plugin-eslint": "^4.5.6",
    "@vue/cli-plugin-router": "^4.5.6",
    "@vue/cli-plugin-vuex": "^4.5.6",
    "@vue/cli-service": "^4.5.6",
    "@vue/eslint-config-prettier": "^6.0.0",
    "babel-eslint": "^10.1.0",
    "babel-plugin-dynamic-import-node": "^2.3.3",
    "babel-plugin-transform-imports": "^2.0.0",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "body-parser": "^1.19.0",
    "chalk": "^4.1.0",
    "chokidar": "^3.4.2",
    "commitizen": "^4.2.2",
    "commitlint": "^11.0.0",
    "commitlint-config-cz": "^0.13.2",
    "compression-webpack-plugin": "^6.0.2",
    "conventional-changelog-cli": "^2.1.1",
    "cz-customizable": "^6.3.0",
    "eslint": "^6.8.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-vue": "^6.2.2",
    "husky": "^4.3.0",
    "jscpd": "^3.3.21",
    "jsinspect": "^0.12.7",
    "lint-staged": "^10.4.0",
    "mockjs": "^1.1.0",
    "prettier": "^2.2.1",
    "sass": "^1.26.11",
    "sass-loader": "^8.0.2",
    "script-ext-html-webpack-plugin": "^2.1.4",
    "standard-version": "^9.0.0",
    "stylelint": "^13.8.0",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-standard": "^20.0.0",
    "vue-cli-plugin-webpack-bundle-analyzer": "^2.0.0",
    "vue-template-compiler": "^2.6.12"
  },
  "husky": {
    "pre-commit": "lint-staged",
    "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
  },
  "lint-staged": {
    "*.{js,vue}": [
      "npm run lint",
      "git add"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-customizable"
    },
    "cz-customizable": {
      "config": "./.cz-config.js"
    }
  },
  "standard-version": {
    "scripts": {
      "posttag": "git push --follow-tags"
    },
    "skip": {
      "tag": true
    }
  },
  "bugs": {
    "url": "https://github.com/GATING/vue-template/issues"
  },
  "homepage": "https://github.com/GATING/vue-template#readme"
}
